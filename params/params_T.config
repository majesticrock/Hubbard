# phases, modes
compute_what phases
improved_boundaries false
# T, U, V
model_parameters 0 0.025 0
# chain, square, cube
lattice_type cube
use_DOS true
# Number of discretization points for the iterator that is being split to MPI
global_iterator_steps 256
global_iterator_upper_limit 0.75
global_iterator_type U
# Number of discretization points for the second iterator
second_iterator_steps 1
second_iterator_upper_limit 2
second_iterator_type V
# Half the number of points in k-space per direction
k_discretization 100
output_folder T0/
use_broyden true
start_basis_at -1
number_of_basis_terms 10